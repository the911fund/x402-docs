{
  "x402Version": "1.0",
  "name": "AI Intelligence API",
  "description": "AI-powered trading intelligence for autonomous agents. USDC payments on Base, Ethereum, and Solana.",
  "documentation": "https://github.com/the911fund/x402-docs",
  "networks": ["base", "ethereum", "solana"],
  "paymentAsset": "USDC",
  "resources": [
    {
      "path": "/alpha/token",
      "method": "GET",
      "description": "Token analysis with price, volume, market cap, and optional Twitter sentiment",
      "priceUSD": 0.05,
      "addons": [
        {
          "name": "twitter",
          "priceUSD": 0.05,
          "description": "Include Twitter sentiment data"
        }
      ],
      "params": ["address", "token", "symbol", "chain", "twitter"]
    },
    {
      "path": "/alpha/trending",
      "method": "GET",
      "description": "Trending tokens and narratives across crypto",
      "priceUSD": 0.05,
      "addons": [
        {
          "name": "twitter",
          "priceUSD": 0.05,
          "description": "Include Twitter trend data"
        }
      ],
      "params": ["category", "limit", "twitter"]
    },
    {
      "path": "/alpha/sentiment",
      "method": "GET",
      "description": "Market sentiment analysis with Twitter data included",
      "priceUSD": 0.08,
      "params": ["query", "topic", "token"]
    },
    {
      "path": "/alpha/search",
      "method": "GET",
      "description": "Neural search across crypto intelligence sources (Exa + AI)",
      "priceUSD": 0.05,
      "params": ["query", "q"]
    },
    {
      "path": "/alpha/deep",
      "method": "GET",
      "description": "Deep research with multi-source analysis (Exa + Firecrawl + Claude)",
      "priceUSD": 0.15,
      "params": ["query", "topic", "token", "url"]
    },
    {
      "path": "/alpha/prediction",
      "method": "GET",
      "description": "Prediction market intelligence from Polymarket + Kalshi with AI synthesis",
      "priceUSD": 0.05,
      "params": ["query", "category"]
    },
    {
      "path": "/alpha/macro",
      "method": "GET",
      "description": "Macro economic intelligence from FRED + prediction markets with AI synthesis",
      "priceUSD": 0.08,
      "params": ["theme"]
    },
    {
      "path": "/alpha/news",
      "method": "GET",
      "description": "AI-filtered crypto news from major publications (CoinTelegraph, Decrypt, CoinDesk, Blockworks) + Twitter",
      "priceUSD": 0.03,
      "params": ["token"]
    }
  ],
  "mcp": {
    "url": "https://x402.911fund.io/mcp",
    "transport": "streamable-http",
    "method": "POST",
    "tools": 9,
    "chains": ["base", "solana"],
    "pricing": {
      "alpha_token": 0.05,
      "alpha_trending": 0.05,
      "alpha_sentiment": 0.08,
      "alpha_search": 0.05,
      "alpha_deep": 0.15,
      "alpha_prediction": 0.05,
